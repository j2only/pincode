{
    "name": "@joseph2/vue-pincode",
    "version": "0.3.4",
    "type": "module",
    "description": "Neumorphism styled pincode input field for Vue.js 3 + Vite + Typescript",
    "author": {
        "name": "Anton Zolotov",
        "email": "antonzolotov@outlook.com"
    },
    "files": [
        "dist"
    ],
    "main": "dist/vite-pincode.js",
    "module": "dist/vue-slide-unlock.es.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --p ./tsconfig.build.json && vite build",
        "build:demo": "vue-tsc && vite build --config vite.config.demo.ts",
        "preview": "vite preview",
        "lint": "run lint:js && run lint:style",
        "lint:js": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
        "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore --fix --config ./.stylelintrc-format"
    },
    "dependencies": {
        "vue": "^3.4.15"
    },
    "devDependencies": {
        "@stylistic/stylelint-plugin": "^2.0.0",
        "@types/eslint": "^8.56.2",
        "@types/node": "^20.11.16",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/eslint-config-standard": "^8.0.1",
        "@vue/eslint-config-typescript": "^12.0.0",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.21.1",
        "postcss-html": "^1.6.0",
        "sass": "^1.70.0",
        "stylelint": "^16.2.1",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard-scss": "^13.0.0",
        "stylelint-order": "^6.0.4",
        "stylelint-scss": "^6.1.0",
        "typescript": "^5.2.2",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.2",
        "vue-tsc": "^1.8.27"
    },
    "packageManager": "yarn@4.1.0",
    "license": "MIT",
    "homepage": "https://weslinkde.github.io/vue-pincode/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joseph2/vue-pincode.git"
    },
    "bugs": {
        "url": "https://github.com/joseph2/vue-pincode/issues"
    },
    "keywords": [
        "pincode",
        "pin",
        "pin code",
        "code",
        "vue",
        "vue3",
        "vue.js",
        "typescript",
        "vue-pincode",
        "weslink",
        "joseph2",
        "neumorphism"
    ]
}